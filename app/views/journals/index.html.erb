
<h1>Journals</h1>

<table>
  <thead>
    <tr>
      <th>Identifier</th>
      <th>Editor</th>
      <th>Publisher</th>
      <th>Indexing</th>
      <th>Copyright</th>
      <th>Subject</th>
      <th>Others</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @journals.each do |journal| %>
      <tr>
        <td><%= journal.identifier %></td>
        <td><%= journal.editor %></td>
        <td><%= journal.publisher %></td>
        <td><%= journal.indexing %></td>
        <td><%= journal.copyright %></td>
        <td><%= journal.subject %></td>
        <td><%= journal.others %></td>
        <td><%= journal.user %></td>
        <td><%= link_to 'Show', journal %></td>
        <td><%= link_to 'Edit', edit_journal_path(journal) %></td>
        <td><%= link_to 'Destroy', journal, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Journal', new_journal_path %>
