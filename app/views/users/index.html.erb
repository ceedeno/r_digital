<div class="container">
  <div class="row">
    <h3>Users</h3>
      <table class="table table-hover">
        <thead>
        <tr>
          <th>Nombre</th>
          <th>Correo</th>
          <th>Opciones</th>

        </tr>
        </thead>


        <tbody>
          <% @users.each do |user| %>
            <tr>
              <%= render user %>
            </tr>
          <% end %>
        </tbody>
      </table>
  </div>
</div>
