<div class="container">
  <div class="row">
    <h3>Usuarios</h3>
      <table class="table table-hover">
        <thead>
        <tr>
          <th>Nombres y Apellidos</th>
          <th>Correo</th>
          <th>Rol</th>
          <th colspan="3">Opciones</th>


        </tr>
        </thead>


        <tbody>
          <% @users.each do |user| %>
            <tr>
              <%= render user %>
            </tr>
          <% end %>
        </tbody>
      </table>
  </div>
</div>
